% length
\newcommand{\km}{\text{ km}}
\newcommand{\kmn}{\text{km}}
\newcommand{\m}{\text{ m}}
\newcommand{\mn}{\text{m}}
\newcommand{\cm}{\text{ cm}}
\newcommand{\mm}{\text{ mm}}

\newcommand{\ft}{\text{ ft}}
\newcommand{\inch}{\text{ in}}

% mass
\newcommand{\kg}{\text{ kg}}
\newcommand{\kgn}{\text{kg}}
\newcommand{\g}{\text{ g}}
\newcommand{\gn}{\text{g}}

\newcommand{\oz}{\text{ oz}}
\newcommand{\ozn}{\text{oz}}

% time
\newcommand{\s}{\text{ s}}
\newcommand{\sn}{\text{s}}
\renewcommand{\min}{\text{ min}}
\newcommand{\minn}{\text{min}}
\newcommand{\hr}{\text{ hr}}
\newcommand{\hrn}{\text{hr}}

% temperature
\newcommand{\C}{^\circ\text C}
\newcommand{\F}{^\circ\text F}

% time derivatives of position
\newcommand{\ms}{\m/\sn}
\newcommand{\mss}{\m/\sn^2}
\newcommand{\kmhr}{\km/\hrn}
\newcommand{\kmhrhr}{\km/\hrn^2}

% mass flow rate
\newcommand{\kgps}{\kg/\sn}

% force
\newcommand{\N}{\text{ N}}

% spring constant
\newcommand{\Npm}{\N/\mn}

% momentum
\newcommand{\Ns}{\N\sn}

% energy
\newcommand{\J}{\text{ J}}

% angle
\newcommand{\rad}{\text{ rad}}
\newcommand{\rev}{\text{ rev}}

% angular velocity
\newcommand{\rpm}{\text{ rpm}}

% torque
\newcommand{\Nm}{\N\mn}

% power
\newcommand{\W}{\text{ W}}
\newcommand{\hp}{\text{ hp}}

% density
\newcommand{\kgmmm}{\kg/\mn^3}

% pressure
\newcommand{\Pa}{\text{ Pa}}
\newcommand{\kPa}{\text{ kPa}}
\newcommand{\atm}{\text{ atm}}
\newcommand{\mmHg}{\text{ mm Hg}}

% frequency
\newcommand{\Hz}{\text{ Hz}}
